<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <data>
    
        <record id="gym_food_type_view_tree" model="ir.ui.view">
            <field name="name">gym.food.type.view.tree</field>
            <field name="model">gym.food.type</field>
            <field name="arch" type="xml">
                <tree editable="bottom">
                    <field name="name"/>
                </tree>
            </field>
        </record>

        <record id="gym_food_type_action" model="ir.actions.act_window">
            <field name="name">Food type</field>
            <field name="res_model">gym.food.type</field>
            <field name="view_type">form</field>
            <field name="view_mode">tree</field>
        </record>

        <menuitem
            id="gym_food_type_menu"
            name="Food Type"
            action="gym_food_type_action"
            parent="nutrition_root_menu"
            sequence="20"/>
    
    </data>
    

</odoo>
